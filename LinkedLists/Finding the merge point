int findMergeNode(SinglyLinkedListNode* head1, SinglyLinkedListNode* head2) {
    
    SinglyLinkedListNode *p=head1, *q=head2;
    while(p!=q) {
        if(p->next == NULL)          
            p=head2;
        else 
            p=p->next;
        if(q->next == NULL)
            q=head1;
        else 
            q=q->next;
    }
    return q->data;
}
